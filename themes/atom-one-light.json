    "tokenColors": [
      {
        "name": "Comment",
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#A0A1A7"
        }
      },
      {
        "name": "Constant",
        "scope": "constant",
        "settings": {
          "foreground": "#986801"
        }
      },
      {
        "name": "Entity",
        "scope": "entity",
        "settings": {
          "foreground": "#50A14F"
        }
      },
      {
        "name": "Invalid",
        "scope": "invalid",
        "settings": {
          "foreground": "#E45649"
        }
      },
      {
        "name": "Keyword",
        "scope": "keyword",
        "settings": {
          "foreground": "#A626A4"
        }
      },
      {
        "name": "Storage",
        "scope": "storage",
        "settings": {
          "foreground": "#A626A4"
        }
      },
      {
        "name": "String",
        "scope": "string",
        "settings": {
          "foreground": "#50A14F"
        }
      },
      {
        "name": "Support",
        "scope": "support",
        "settings": {
          "foreground": "#0184BC"
        }
      },
      {
        "name": "Variable",
        "scope": "variable",
        "settings": {
          "foreground": "#E45649"
        }
      },
      {
        "name": "Markup Heading",
        "scope": ["markup.heading", "entity.name.section"],
        "settings": {
          "foreground": "#4078F2"
        }
      },
      {
        "name": "Markup Bold",
        "scope": "markup.bold",
        "settings": {
          "fontStyle": "bold",
          "foreground": "#A626A4"
        }
      },
      {
        "name": "Markup Italic",
        "scope": "markup.italic",
        "settings": {
          "fontStyle": "italic",
          "foreground": "#A626A4"
        }
      },
      {
        "name": "Markup Link",
        "scope": "markup.underline.link",
        "settings": {
          "foreground": "#4078F2"
        }
      },
      {
        "name": "Markup Quote",
        "scope": "markup.quote",
        "settings": {
          "foreground": "#50A14F",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Markup Code",
        "scope": "markup.inline.raw",
        "settings": {
          "foreground": "#0184BC"
        }
      },
      {
        "name": "Punctuation",
        "scope": "punctuation",
        "settings": {
          "foreground": "#383A42"
        }
      },
      {
        "name": "Meta",
        "scope": "meta",
        "settings": {
          "foreground": "#383A42"
        }
      },
      {
        "name": "HTML Tag",
        "scope": ["entity.name.tag", "meta.tag.sgml"],
        "settings": {
          "foreground": "#E45649"
        }
      },
      {
        "name": "HTML Attribute",
        "scope": "entity.other.attribute-name",
        "settings": {
          "foreground": "#986801"
        }
      },
      {
        "name": "JavaScript Variable Parameter",
        "scope": [
          "variable.parameter",
          "meta.parameter"
        ],
        "settings": {
          "foreground": "#383A42"
        }
      },
      {
        "name": "JavaScript/TypeScript Class",
        "scope": [
          "entity.name.type.class",
          "entity.name.class"
        ],
        "settings": {
          "foreground": "#4078F2"
        }
      },
      {
        "name": "JavaScript/TypeScript Function",
        "scope": [
          "entity.name.function",
          "support.function"
        ],
        "settings": {
          "foreground": "#4078F2"
        }
      },
      {
        "name": "JavaScript/TypeScript Object Property",
        "scope": [
          "meta.object-literal.key",
          "variable.other.property",
          "support.variable.property"
        ],
        "settings": {
          "foreground": "#0184BC"
        }
      },
      {
        "name": "JavaScript/TypeScript Object",
        "scope": "variable.other.object",
        "settings": {
          "foreground": "#383A42"
        }
      },
      {
        "name": "CSS Property Name",
        "scope": "support.type.property-name.css",
        "settings": {
          "foreground": "#0184BC"
        }
      },
      {
        "name": "CSS Property Value",
        "scope": [
          "support.constant.property-value.css",
          "support.constant.color.w3c-standard-color-name.css"
        ],
        "settings": {
          "foreground": "#50A14F"
        }
      },
      {
        "name": "CSS Selector",
        "scope": [
          "entity.name.tag.css",
          "entity.other.attribute-name.class.css",
          "entity.other.attribute-name.id.css",
          "entity.other.attribute-name.pseudo-class.css"
        ],
        "settings": {
          "foreground": "#E45649"
        }
      },
      {
        "name": "Python Function Parameter",
        "scope": "variable.parameter.function.python",
        "settings": {
          "foreground": "#986801"
        }
      },
      {
        "name": "Python Function Call",
        "scope": "meta.function-call.python",
        "settings": {
          "foreground": "#4078F2"
        }
      },
      {
        "name": "Python Function Arguments",
        "scope": "meta.function-call.arguments.python",
        "settings": {
          "foreground": "#383A42"
        }
      },
      {
        "name": "Ruby Class",
        "scope": [
          "entity.name.type.class.ruby",
          "entity.name.class.ruby"
        ],
        "settings": {
          "foreground": "#4078F2"
        }
      },
      {
        "name": "Ruby Module",
        "scope": "entity.name.type.module.ruby",
        "settings": {
          "foreground": "#4078F2"
        }
      },
      {
        "name": "Ruby Symbol",
        "scope": "constant.language.symbol.ruby",
        "settings": {
          "foreground": "#0184BC"
        }
      }
    ]
  }